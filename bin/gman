#!/usr/bin/env node

;(function() {
    'use strict';

    var program = require('commander');

    program
        .version('0.0.1')
        .command('api', 'Run the REST API server')
        .command('web', 'Run the Express Web Server')
        .command('tool', 'Run tool')
        .command('csbot', 'Run CSBOT')
        ;

    program.parse(process.argv);

}());
